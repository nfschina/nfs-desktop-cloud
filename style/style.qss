/*标题栏关闭按钮设置*/
QMainWindow#HomeWindow>QWidget#TitleBar>QPushButton#ButtonClose {
border-image: url(:/new/index/close);

}
QMainWindow#HomeWindow>QWidget#TitleBar>QPushButton#ButtonClose:hover {
border-image: url(:/new/index/close_hover);

}
