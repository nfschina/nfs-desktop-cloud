QMainWindow#HomeWindow>QWidget#TitleBar>QPushButton#ButtonMin {
border-image: url(:/new/index/image/MINUS.png);

}
QMainWindow#HomeWindow>QWidget#TitleBar>QPushButton#ButtonMin:hover {
